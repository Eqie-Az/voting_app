RewriteEngine On

# Abaikan file statis (CSS, JS, gambar, font, dll)
RewriteCond %{REQUEST_URI} !\.(css|js|png|jpg|jpeg|gif|ico|woff|woff2|ttf|svg)$ [NC]

# Abaikan file & folder yang sudah ada
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d

# Arahkan semua request lain ke index.php dengan parameter url
RewriteRule ^(.*)$ index.php?url=$1 [QSA,L]
